// mode: Medium
// String: Rabin Carp

vector<int> stringMatch(string &str, string &pat) {
    int patSize = pat.size();
    int lastIndex = str.size() - patSize;
    
    vector<int> ans;
    for(int i=0; i<=lastIndex; i++)
        if(str[i]==pat[0] && str.substr(i,patSize)==pat)
            ans.push_back(i);
    
    return ans;
}
